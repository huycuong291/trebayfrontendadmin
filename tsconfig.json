{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/constants/*": [
        "src/constants/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/redux/*": [
        "src/redux/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/context/*": [
        "src/context/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}